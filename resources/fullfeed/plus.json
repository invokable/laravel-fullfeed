[
    {
        "name": "Laravel Official Blog",
        "data": {
            "url": "^https:\/\/laravel\\.com\/blog\/",
            "selector": "article"
        }
    },
    {
        "name": "PR TIMES",
        "data": {
            "url": "^https:\/\/prtimes\\.jp\/main\/html\/rd\/p\/",
            "selector": "div#press-release-body"
        }
    },
    {
        "name": "Qiita",
        "data": {
            "url": "^https:\/\/qiita\\.com\/(.+)\/items\/",
            "selector": "article > section"
        }
    },
    {
        "name": "Togetter",
        "data": {
            "url": "^https:\/\/togetter\\.com\/li\/",
            "selector": "section.entry_main",
            "after_callable": [
                "Revolution\\Fullfeed\\Extractor\\StripTags:a,div,p"
            ]
        }
    },
    {
        "name": "note",
        "data": {
            "url": "^https:\/\/note\\.com\/",
            "selector": "div[data-name=\"body\"]"
        }
    },
    {
        "name": "zenn",
        "data": {
            "url": "^https:\/\/zenn\\.dev\/(.+)\/articles\/",
            "selector": "div.znc"
        }
    },
    {
        "name": "朝日新聞",
        "data": {
            "url": "^https:\/\/www\\.asahi\\.com\/articles\/",
            "selector": "main#main",
            "after_callable": [
                "Revolution\\Fullfeed\\Extractor\\StripTags:a,div,p,img,figure,h2",
                "Revolution\\Fullfeed\\Extractor\\RemoveElements"
            ],
            "remove": [
                "div.notPrint",
                "a[role=\"button\"]",
                "div[x-component-name]"
            ]
        }
    },
    {
        "name": "毎日新聞",
        "data": {
            "url": "^https:\/\/mainichi\\.jp\/articles\/",
            "selector": "section#articledetail-body",
            "after_callable": [
                "Revolution\\Fullfeed\\Extractor\\RemoveElements"
            ],
            "remove": [
                "script"
            ]
        }
    },
    {
        "name": "産経ニュース",
        "data": {
            "url": "^https:\/\/www\\.sankei\\.com\/article\/",
            "selector": "div.article-body",
            "after_callable": [
                "Revolution\\Fullfeed\\Extractor\\RemoveElements"
            ],
            "remove": [
                "script"
            ]
        }
    },
    {
        "name": "読売新聞",
        "data": {
            "url": "^https:\/\/www\\.yomiuri\\.co\\.jp\/",
            "selector": "article.article-content > div.p-main-contents",
            "after_callable": [
                "Revolution\\Fullfeed\\Extractor\\RemoveElements"
            ],
            "remove": [
                "script",
                "aside"
            ]
        }
    }
]