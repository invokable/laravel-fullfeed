[
    {
        "name": "note",
        "data": {
            "url": "^https://note\\.com/",
            "selector": "div[data-name=\"body\"]"
        }
    },
    {
        "name": "zenn",
        "data": {
            "url": "^https://zenn\\.dev\/(.+)\/articles\/",
            "selector": "div.znc"
        }
    },
    {
        "name": "Qiita",
        "data": {
            "url": "^https://qiita\\.com\/(.+)/items\/",
            "selector": "article > section"
        }
    },
    {
        "name": "Laravel Official Blog",
        "data": {
            "url": "^https://laravel\\.com\/blog\/",
            "selector": "article"
        }
    },
    {
        "name": "Togetter",
        "data": {
            "url": "^https://togetter\\.com\/li\/",
            "selector": "section.entry_main",
            "callable": ["Revolution\\Fullfeed\\Extractor\\TogetterExtractor"],
            "remove": [
                "div.list_photo_box",
                "img",
                "svg",
                "use",
                "button",
                "span.intent",
                "div.pagenation",
                "script"
            ],
            "after_callable": ["Revolution\\Fullfeed\\Extractor\\ReplaceMatches"],
            "replace": [
                {
                    "pattern": "/ data-(h-)?index=\"[0-9]+\"/",
                    "replace": ""
                },
                {
                    "pattern": "/ data-id=\"[0-9]+\"/",
                    "replace": ""
                }
            ]
        }
    }
]
